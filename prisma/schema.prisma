generator client {
    provider = "prisma-client-js"
    output = "../src/generated/prisma"
}

datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
}

model Product {
    id Int @id @default(autoincrement())
    name String
    image String @default("default.jpg")
    price Decimal @bd.Money()
    ingredients String?
}